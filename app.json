{
  "expo": {
    "name": "PLANLY",
    "slug": "planly",
    "version": "1.1.1",
    "orientation": "portrait",
    "icon": "./assets/graphics/icon.png",
    "splash": {
      "image": "./assets/graphics/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#A8383B"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/graphics/adaptive-icon.png",
        "backgroundColor": "#A8383B"
      },
      "package": "ir.solacode.planly"
    },
    "plugins": [
      "expo-secure-store",
      "expo-localization",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "9c94596a-82eb-45dc-9d2e-60dccade2f83"
      }
    },
    "owner": "solacode"
  }
}
