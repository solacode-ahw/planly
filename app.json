{
  "expo": {
    "name": "PLANLY",
    "slug": "PLANLY",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/graphics/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/graphics/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#A8383B"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/graphics/adaptive-icon.png",
        "backgroundColor": "#A8383B"
      }
    },
    "plugins": [
      "expo-secure-store",
      "expo-localization",
      "expo-font"
    ]
  }
}
